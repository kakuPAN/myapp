<h1>ログインページ</h1>
<div>
<%= form_with url: login_path do |f| %>
    <%= f.label :email, "メールアドレス" %>
    <%= f.email_field :email %>
    <%= f.label :password, "パスワード" %>
    <%= f.password_field :password%> 
  <%= f.submit %>
</div>
<div>
  <p>アカウントをお持ちではない方は<%= link_to "こちら", new_user_path %></p>
</div>
<% end %>